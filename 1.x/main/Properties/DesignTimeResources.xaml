<ResourceDictionary
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:model="clr-namespace:Awful.Models;assembly=Awful" 
	xmlns:Awful_SampleData="clr-namespace:Awful.SampleData"
	xmlns:kollasoft="clr-namespace:KollaSoft;assembly=KollaSoft.WP7">
	<ResourceDictionary.MergedDictionaries>
		<ResourceDictionary Source="/KollaSoft.WP7;component/KSTemplates.xaml"/>
		<ResourceDictionary Source="/Awful;component/AwfulAssets.xaml"/>
	</ResourceDictionary.MergedDictionaries>
	<!-- Resource dictionary entries should be defined here. -->
	<model:AwfulSmiley x:Key="sampleSmiley" Text=":sample:" Title="sample" Uri="icons/appbar.close.rest.png"/>
	<Awful_SampleData:SampleLoopingListSource x:Key="sampleLoopSource"/>
	<kollasoft:KSStringList x:Key="sampleList"/>
	<model:SAThread x:Key="sampleThread" ThreadTitle="sample thread title" AuthorName="sample author" NewPostCount="99"/>
	<Awful_SampleData:SampleSmileyData x:Key="sampleSmilies"/>
	<Awful_SampleData:SampleSmileyPages x:Key="sampleSmileyPages"/>
</ResourceDictionary>